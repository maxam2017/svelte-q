{
  "name": "svelte-q",
  "description": "svelteQ is a data fetching library built for Svelte",
  "author": "maxam2017",
  "version": "0.4.2-0",
  "license": "MIT",
  "type": "module",
  "exports": "./dist/svelte-q.esm.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "svelte",
    "request",
    "fetch",
    "cache",
    "query"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch --format esm",
    "build": "tsdx build --format esm",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage",
    "lint": "tsdx lint",
    "lint:fix": "tsdx lint --fix",
    "prepare": "rm -rf .husky && husky install && npx husky add .husky/pre-commit \"pnpm lint:fix\" && tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "useTabs": false,
    "singleQuote": true
  },
  "size-limit": [
    {
      "path": "dist/svelte-q.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/svelte-q.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/qs": "^6.9.7",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.2",
    "size-limit": "^7.0.8",
    "svelte": "^3.48.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "svelte": "^3.48.0"
  },
  "dependencies": {}
}
